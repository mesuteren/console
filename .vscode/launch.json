    "version": "0.2.0",
    "configurations": [

        {
            "name": ".Net Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net5.0/console.dll",
            "args":[],
            "cwd": "${workspaceFolder}",
            "console": "externalTerminal",
            "stopAtEntry": false

        },
        {
            "name": ".Net Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId":"${command:pickProcess}"
        }
        


    ]
}